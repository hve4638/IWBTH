                troom = rmstage01a;
            break;
            
            case rmstage02a:
            case rmstage02b:
                tx = -1; ty = -1;
                troom = rmstage02a;
            break;
            
            case rmstage03a:
            case rmstage03b:
            case rmstage03c:
                tx = -1; ty = -1;
                troom = rmstage03a;
            break;
            
            
